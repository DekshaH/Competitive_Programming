//https://leetcode.com/problems/jewels-and-stones/

class Solution {
public:
    int numJewelsInStones(string J, string S) {
        int i,j,count=0;
        for(i=0;i<J.size();i++)
            for(j=0;j<S.size();j++)
                if(J[i]==S[j])
                    count++;
        return(count);
        
    }
};
